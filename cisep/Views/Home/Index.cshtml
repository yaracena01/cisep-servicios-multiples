@section scripts {
    <script>
        var cards = $('.cards-2 .card-body');
        var maxHeight = 0;
        for (var i = 0; i < cards.length; i++) {
            if (maxHeight < $(cards[i]).outerHeight()) {
                maxHeight = $(cards[i]).outerHeight() - 50;
            }
        }
        for (var i = 0; i < cards.length; i++) {
            $(cards[i]).height(maxHeight);
        }
        function cformError() {
            $("#emailContact").removeClass().addClass('shake animated').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
                $(this).removeClass();
            });
        }
        $('#cemail')
            .on('invalid', function () {
                cformError();
                return this.setCustomValidity('@Localizer["Please Enter valid email"]');
            })
            .on('input', function () {
                return this.setCustomValidity('');
            });

        $('#cname')
            .on('invalid', function () {
                cformError();
                return this.setCustomValidity('@Localizer["This field is required"]');
            })
            .on('input', function () {
                return this.setCustomValidity('');
            });
        $('#cmessage')
            .on('invalid', function () {
                cformError();
                return this.setCustomValidity('@Localizer["This field is required"]');
            })
            .on('input', function () {
                return this.setCustomValidity('');
            });
        $('#cterms')
            .on('invalid', function () {
                cformError();
                return this.setCustomValidity('@Localizer["Click this box if you want to continue"]');
            })
            .on('input', function () {
                return this.setCustomValidity('');
            });

        function validate_email (email) {
            var emailExp = new RegExp(/^\b[A-Z0-9._%+-]+@@[A-Z0-9.-]+\.[A-Z]{2,4}\b$/i);
            return emailExp.test(email);
        }

        $('#emailContact').submit(function (e) {
                e.preventDefault();
                var subject = $("[name='Subject']").val(),
                    to = $("[name='To']").val(),
                    body = $("[name='Body']").val();
            if ((subject != null && $.trim(subject) != "") && (to != null && $.trim(to) != "" && validate_email(to)) && (body != null && $.trim(body) != "") && $('input.checkbox_check').is(':checked')) {
                $(".spinner-wrapper").fadeIn();
                $.ajax({
                    type: "post",
                    url: "@Url.Action("Email", "Home")",
                    data: {
                        Subject: subject,
                        To: to,
                        Body: body
                    },
                    success: function (response) {
                        if (response.success == true) {
                            $(".spinner-wrapper").fadeOut();
                            toastr.info('@Localizer["Your email has been sent successfully."]', '', {
                                "positionClass": "toast-bottom-left",
                                "showDuration": "300",
                                "hideDuration": "1000",
                                "timeOut": "5000",
                                "extendedTimeOut": "1000",
                                "showEasing": "swing",
                                "hideEasing": "linear",
                                "showMethod": "fadeIn",
                                "hideMethod": "fadeOut"
                            });
                            $("[name='Subject']").val("");
                            $("[name='To']").val("");
                            $("[name='Body']").val("");
                        } else {
                            $(".spinner-wrapper").fadeOut();
                            toastr.info(response.message, '', {
                                "positionClass": "toast-bottom-left",
                                "showDuration": "300",
                                "hideDuration": "1000",
                                "timeOut": "5000",
                                "extendedTimeOut": "1000",
                                "showEasing": "swing",
                                "hideEasing": "linear",
                                "showMethod": "fadeIn",
                                "hideMethod": "fadeOut"
                            });

                        }
                    },
                    error: function () {

                    }
                });
            }
        });
        $('.idiom').on("click", function (e) {
            e.preventDefault();
            $me = $(this);
            var idiom = $me.data("idiom");
                    //$(".spinner-wrapper").fadeIn();
                    $.ajax({
                        type: "post",
                        url: "@Url.Action("SetLanguage", "Home")",
                        data: {
                            culture: idiom,
                            returnUrl: "/"
                        },
                        success: function (response) {
                            console.log(response);
                            if (response.success == true) {
                                //$(".spinner-wrapper").fadeOut();
                                location.reload();
                            } else {
                                $(".spinner-wrapper").fadeOut();
                                toastr.info(response.message, '', {
                                    "positionClass": "toast-bottom-left",
                                    "showDuration": "300",
                                    "hideDuration": "1000",
                                    "timeOut": "5000",
                                    "extendedTimeOut": "1000",
                                    "showEasing": "swing",
                                    "hideEasing": "linear",
                                    "showMethod": "fadeIn",
                                    "hideMethod": "fadeOut"
                                });
                            }
                        },
                        error: function () {

                        }
                    });

        });
        $.localization = true
        function getValues(model) {
            $('#project-1 .imgs').attr('src', 'imageServices/' + model.photo);
            $('#project-1 .tls').html(model.name);
            $('#project-1 .prs').html(model.description);
            var detailServices = "";
            
            for (var s of model.services_Details) {
                detailServices += "<li class='media'><i class='fas fa-square'></i><div class='media-body'>" + s.name + "</div></li >"
            }
            $('#project-1 .ds').html(detailServices);
        }
    </script>
}
