                </div>
            </div>
        </div>
        <div class="services row">
            <div class="col-lg-12">
                <div class="slider-container">
                    <div class="swiper-container card-slider">
                        <div class="swiper-wrapper">
                            @foreach (var row in ViewBag.services)
                            {
                            <div class="swiper-slide">
                                <div class="card">
                                    <div class="card-image element-item">
                                        <img class="img-fluid" src="~/imageServices/@row.Photo" alt="alternative">
                                    </div>
                                    <div class="card-body">
                                        <h3 class="card-title">@row.Name</h3>
                                        <p style="white-space: pre-line;color:black">@row.Description</p>
                                        <ul class="list-unstyled li-space-lg" style="text-align:left">
                                            @foreach (var sd in row.Services_Details)
                                            {
                                                i++;                                             
                                            <li class="media">
                                                <i class="fas fa-square"></i>
                                                <div class="media-body">@sd.Name</div>
                                            </li>
                                                    if (i == 5)
                                                    {
                                                        i = 0;
                                                        break;
                                                    }
                                                }
                                        </ul>
                                        <p>           
                                            <a style="font-size: 12px;font-weight: bold;" class="popup-with-move-anim" id="model" onclick='getValues(@Json.Serialize(row));' href="#project-1">
                                                <div class="element-item-overlay"><span>@Localizer["More Information"]</span></div>
                                            </a>
                                        </p>
                                    </div>
                                    @if (row.Url != "" && row.Url != null)
                                     {
                                <div class="button-container">
                                    @if (row.Id != 2060)
                                    {
                                        @Html.ActionLink(booknow, "PayServices", "Home", new { id = row.Id }, new { target = "_blank", @class = "btn-solid-reg page-scroll" })
                                    }
                                    else
                                    {
                                        <a class="btn-solid-reg page-scroll" target="_blank" href="/ServicesPage/CreditRepairIndex">@row.UrlName</a>
                                    }


                                </div>
                                     }
                                </div>
                            </div>
                            }
                        </div>
                        <div class="swiper-button-next"></div>
                        <div class="swiper-button-prev"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

